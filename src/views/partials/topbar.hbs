<div class="uk-grid top-nav">
    <div class="uk-width-1-1">
        <nav class="top-bar" data-topbar>
            <div class="title-area uk-float-left">
                <div class="logo">
                   
                 
                   
              
                </div>
            </div>

            <section class="top-bar-section uk-clearfix">
                <!-- Right Nav Section -->
                <div class="top-menu uk-float-right">
                    <ul class="uk-subnav uk-margin-bottom-remove">
                        <!--<li class="top-bar-icon"><a href="/tickets/create/"><i class="material-icons">&#xE145;</i></a></li>-->
                        {{#canUser data.user "ticket:create"}}
                            <li class="top-bar-icon nopadding tour-addTicket"><button class="anchor" data-uk-modal="{target:'#ticketCreateModal',bgclose:false}"><i class="material-icons">&#xE145;</i></button></li>
                            <li class="top-bar-icon nopadding">
                                <i class="material-icons">more_vert</i>
                            </li>
                        {{/canUser}}
                        <li class="top-bar-icon tour-messagesNotification">
                            <a id="btn_mail-notifications" href="#" class="no-ajaxy uk-vertical-align" data-notifications="mail-notifications"><i class="material-icons">email</i>
                                <!--<span class="alert uk-border-circle label"></span>-->
                            </a>
                        </li>
                        <li class="top-bar-icon tour-ticketNotifications">
                            <a id="btn_notifications" href="#" class="no-ajaxy uk-vertical-align" data-updateUi="notifications" data-notifications="notifications"><i class="material-icons">&#xE88E;</i>
                                <span class="alert uk-border-circle label {{#compare data.common.notifications.unreadCount "==" 0}}hide{{/compare}} ">{{data.common.notifications.unreadCount}}</span>
                            </a>
                        </li>
                        <li class="top-bar-icon tour-onlineUserListButton">
                            <a href="#online-list-sidebar" class="no-ajaxy" data-updateUi="online-Users" data-uk-offcanvas="{mode: 'slide'}">
                                <svg style="width:24px;height:24px" viewBox="0 0 24 24">
                                    <path d="M9,22A1,1 0 0,1 8,21V18H4A2,2 0 0,1 2,16V4C2,2.89 2.9,2 4,2H20A2,2 0 0,1 22,4V16A2,2 0 0,1 20,18H13.9L10.2,21.71C10,21.9 9.75,22 9.5,22V22H9M16,14V13C16,11.67 13.33,11 12,11C10.67,11 8,11.67 8,13V14H16M12,6A2,2 0 0,0 10,8A2,2 0 0,0 12,10A2,2 0 0,0 14,8A2,2 0 0,0 12,6Z" />
                                </svg>
                                <span class="online-user-count alert uk-border-circle label hide"></span>
                            </a>
                        </li>
                        <li class="profile-area profile-name" data-username="{{data.user.username}}" data-userId="{{data.user._id}}"><span>{{data.user.fullname}}</span>
                            <div style="position: relative; display: inline-block;">
                                <div data-uk-dropdown="{pos: 'bottom-right', mode: 'click'}" aria-haspopup="true" aria-expanded="false">
                                    <a href="#" class="profile-pic no-ajaxy uk-vertical-align-middle">
                                        {{#isNull data.user.image}}
                                            <img id="profileImage" data-userid="{{data.user._id}}" src="/uploads/users/defaultProfile.jpg" alt="Profile"/>
                                        {{else}}
                                            <img id="profileImage" data-userid="{{data.user._id}}" src="/uploads/users/{{data.user.image}}" alt="Profile"/>
                                        {{/isNull}}
                                    </a>
                                    <div class="nopadding-left nopadding-right uk-dropdown uk-dropdown-small uk-margin-top-remove">
                                        <ul class="uk-nav uk-topbar" style="margin: 0 !important;">
                                            <li class="uk-nav-header">{{data.user.fullname}}</li>
                                            <li class="uk-dropdown-close"><a href="/profile" close-uk-dropdown>Profile</a></li>
                                            <li><hr style="margin: 5px 0;" /></li>
                                            <li class="uk-dropdown-close"><a href="/logout">Logout</a></li>
                                        </ul>
                                    </div>
                                </div>
                            </div>
                        </li>
                    </ul>
                    <div id="online-Users" data-scroll="online-Users-List" class="notifications p-dropdown" data-top-offset="-5" data-left-offset="4">
                        <div class="actions"><span style="padding-left: 5px;">Usuarios Online</span>
                            <!--<div class="right"><a class="hoverUnderline" href="#">Settings</a></div>-->
                        </div>
                        <div id="online-Users-List" class="notifications-Messages">
                            <ul>

                            </ul>
                        </div>
                    </div>
                    <div ng-controller="commonCtrl" ng-init="setDefaultCreateTicketValues();loadTour();">
                        {{> noticeAlertWindow}}

                        {{> topbar/notifications }}
                        {{> topbar/messages }}

                    </div>
                </div>
            </section>
        </nav>
    </div>
</div>